<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/inqery_view"
    tools:context="com.example.ls.shoppingmall.user.activity.FamilyActivity">

    <include
        android:id="@+id/top_header"
        layout="@layout/myinformation_header_include" />

    <com.example.ls.shoppingmall.utils.layoututils.OverScrollView
        android:layout_below="@+id/top_header"
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/x8"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/ac_connection_doctor_rl"
                style="@style/userinformation_relativelayout"
                android:layout_marginTop="@dimen/x1">

                <TextView
                    android:textSize="@dimen/x15"
                    android:drawablePadding="@dimen/x8"
                    android:drawableLeft="@drawable/connected_doctor_iv"
                    android:id="@+id/ac_connection_doctor_tv"
                    style="@style/userinformation_text"
                    android:layout_marginLeft="15dp"
                    android:layout_marginStart="15dp"
                    android:text="专家"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/ac_connection_doctor_team_rl"

                style="@style/userinformation_relativelayout"
                android:layout_marginTop="@dimen/x1">

                <TextView
                    android:textSize="@dimen/x15"
                    android:drawablePadding="@dimen/x8"
                    android:drawableLeft="@drawable/collect_team"
                    android:id="@+id/ac_connection_doctor_team_tv"
                    style="@style/setting_text"
                    android:text="医师团" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/ac_connection_doctor_artical_rl"

                style="@style/userinformation_relativelayout"
                android:layout_marginTop="@dimen/x1">

                <TextView
                    android:textSize="@dimen/x15"
                    android:drawablePadding="@dimen/x8"
                    android:drawableLeft="@drawable/connected_artical"
                    android:id="@+id/ac_connection_doctor_artical_tv"
                    style="@style/setting_text"
                    android:text="文章" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/ac_set_clearlurch"
                style="@style/userinformation_relativelayout"
                android:layout_marginTop="@dimen/x1">

                <TextView
                    android:textSize="@dimen/x15"
                    android:drawablePadding="@dimen/x8"
                    android:drawableLeft="@drawable/connected_action_iv"
                    style="@style/setting_text"
                    android:text="案例" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/ac_set_getLocation"
                style="@style/userinformation_relativelayout"
                android:layout_marginTop="@dimen/x1">

                <TextView
                    android:textSize="@dimen/x15"
                    android:drawablePadding="@dimen/x8"
                    android:drawableLeft="@drawable/main_cart_press"
                    style="@style/setting_text"
                    android:text="商品" />

            </RelativeLayout>
        </LinearLayout>
    </com.example.ls.shoppingmall.utils.layoututils.OverScrollView>
</RelativeLayout>
