<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.ls.shoppingmall.community.activity.MedicalResultActivity">

    <include layout="@layout/myinformation_header_include" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/x50"
        android:background="#ffffff">

        <android.support.design.widget.TabLayout
            android:id="@+id/at_medical_result_tb"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x55"
            app:tabGravity="fill"
           />
    </RelativeLayout>
    <android.support.v4.view.ViewPager
        android:layout_marginTop="@dimen/x6"
        android:id="@+id/ac_medical_result_vp"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#ffffff">

    </android.support.v4.view.ViewPager>
</LinearLayout>
